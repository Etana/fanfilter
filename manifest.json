{
  "manifest_version": 2,
  "name": "fanfilter",
  "description": "advanced filter on fanfiction.net",
  "version": "1.0.4",
  "content_scripts": [
    {
      "matches": ["https://www.fanfiction.net/*", "https://www.fictionpress.com/*"],
      "js": ["e.js"]
    }
  ],
  "icons": {"128": "i.png"}
}
